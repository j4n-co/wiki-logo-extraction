	</button>
	<hr>

	<table x-show="tab === 'wikipedia'">
		<thead>
			<tr>
				<td>Wikipedia</td>
				<td>originals ({{wiki.totals.originals}})</td>
				<td>wordmarks ({{wiki.totals.wordmarks}}, {{wiki.totals.wordmarksPer}}%)</td>
				<td>taglines ({{wiki.totals.taglines}}, {{wiki.totals.taglinesPer}}%)</td>
			</tr>
		</thead>
		<tbody>
			{{#each wiki}}
			{{#if original}}
			<tr>
				<td><a href="https://{{@key}}.wikipedia.org">{{@key}}</a></td>
				<td><img loading="lazy" src="{{original}}"/></td>
				<td {{#unless wordmark}}class="empty"{{/unless}}>
					{{#if wordmark}}<img loading="lazy" src="{{wordmark}}"/>{{/if}}
				</td>
				<td {{#unless tagline}}class="empty"{{/unless}}>
					{{#if tagline}}<img loading="lazy" src="{{tagline}}"/>{{/if}}
				</td>
				<td class="demo">
					{{#if wordmark}}
					<div class="logo">
						<div class="left-container">
							<img loading="lazy" class="icon wikipedia" src="./optimized-logos/wikipedia.svg">
						</div>
						<div class="right-container">
							<img loading="lazy" class="wordmark mb-4" src="{{wordmark}}"/>
							<img loading="lazy" class="tagline" src="{{tagline}}"/>
						</div>
					</div>
					{{/if}}
				</td>
			</tr>
			{{/if}}
			{{/each}}
		</tbody>
	</table>


